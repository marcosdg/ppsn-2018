Cell[TextData[StyleBox["The one-max problem simply consists in finding the \
binary string with maximum number of ones, for a given length (i.e. \
dimension).",
 FontFamily->"Latin Modern Roman"]], "Text",
 CellChangeTimes->{{3.726047704376752*^9, 3.726047709456813*^9}, {
  3.7286329145025682`*^9, 
  3.728633035589056*^9}},ExpressionUUID->"ec52f388-4d10-4b61-9175-\
a1650e47fd95"],

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{"fOneMax", ":", " ", 
    RowBox[{
     RowBox[{"the", " ", "fitness", " ", "function", " ", "of", " ", "one"}], 
     "-", "max"}]}], " ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"fOneMax", "[", 
      RowBox[{"dim_", ",", "onesZeros_"}], "]"}], ":=", 
     RowBox[{"Sum", "[", 
      RowBox[{
       RowBox[{"onesZeros", "[", 
        RowBox[{"[", "i", "]"}], "]"}], ",", " ", 
       RowBox[{"{", 
        RowBox[{"i", ",", " ", "1", ",", " ", "dim"}], "}"}]}], "]"}]}], 
    ";"}], "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{"fsOneMax", ":", " ", 
     RowBox[{
     "the", " ", "image", " ", "of", " ", "the", " ", "fitness", " ", 
      "function"}]}], " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"fsOneMax", "[", "dim_", "]"}], ":=", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{"fOneMax", "[", 
         RowBox[{"dim", ",", "#"}], "]"}], "&"}], ")"}], "/@", 
      RowBox[{"Tuples", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"0", ",", "1"}], "}"}], ",", "dim"}], "]"}]}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{"zeroAvgFOneMax", ":", " ", 
     RowBox[{
      RowBox[{"the", " ", "zero"}], "-", 
      RowBox[{"averaged", " ", "fitness", " ", "function"}]}]}], " ", "*)"}], 
   
   RowBox[{
    RowBox[{
     RowBox[{"zeroAvgFOneMax", "[", "dim_", "]"}], ":=", 
     RowBox[{
      RowBox[{"fsOneMax", "[", "dim", "]"}], " ", "-", " ", 
      RowBox[{"Mean", "[", 
       RowBox[{"fsOneMax", "[", "dim", "]"}], "]"}]}]}], ";"}]}]}]], "Input",
 CellChangeTimes->{{3.726047895432748*^9, 3.7260478954407988`*^9}, {
  3.726297289840343*^9, 3.7262973732694473`*^9}, {3.7262981469431477`*^9, 
