Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{"fWP", ":", " ", 
    RowBox[{
    "the", " ", "fitness", " ", "function", " ", "of", " ", "weight", " ", 
     "partition"}]}], " ", "*)"}], 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"fWP", "[", 
      RowBox[{"dim_", ",", "weights_", ",", "onesZeros_"}], "]"}], ":=", 
     RowBox[{
