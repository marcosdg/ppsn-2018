Cell[TextData[{
 StyleBox["In the NAES problem we are given a set ",
  FontFamily->"Latin Modern Roman"],
 Cell[BoxData[
  FormBox[
   StyleBox[
    RowBox[{"{", 
     RowBox[{
      SubscriptBox["x", "1"], ",", "...", ",", 
