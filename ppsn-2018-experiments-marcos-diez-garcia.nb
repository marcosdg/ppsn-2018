      "2"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{"fsWP", ":", " ", 
     RowBox[{
     "the", " ", "image", " ", "of", " ", "the", " ", "fitness", " ", 
      "function"}]}], " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"fsWP", "[", 
      RowBox[{"dim_", ",", "weights_"}], "]"}], ":=", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{"fWP", "[", 
         RowBox[{"dim", ",", "weights", ",", "#"}], "]"}], "&"}], ")"}], "/@", 
